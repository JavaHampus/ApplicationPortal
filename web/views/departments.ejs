  <body>
    <%- include('./css/style'); %>
    <div>
        <%- include('./css/departments'); %>

      <div class="departments-container">
        <div class="departments-container1">
          <h1 class="departments-text">Departments</h1>
          <span class="departments-text01">
            <span>
              Choose a department that you want to apply for. You can view your
            </span>
            <br />
            <span>pending applications in</span>
            <span class="departments-text05"><a href="/@me">My Applications</a></span>
            <br />
          </span>
          <div class="departments-container2">
            <% for (var i = 0; i < departments.length; i++) { %>
                <div class="departments-container5">
                    <img
                        src="<%= departments[i].departmentImage %>"
                        alt="image"
                        loading="lazy"
                        class="departments-image2"
                    />
                    <span class="departments-text11">
                        <span><%= departments[i].name %></span>
                        <br />
                    </span>
                    <button type="button" class="departments-button2 button">
                        <svg
                        viewBox="0 0 804.5714285714286 1024"
                        class="departments-icon4"
                        >
                        <path
                            d="M804.571 420.571v109.714c0 30.286-24.571 54.857-54.857 54.857h-237.714v237.714c0 30.286-24.571 54.857-54.857 54.857h-109.714c-30.286 0-54.857-24.571-54.857-54.857v-237.714h-237.714c-30.286 0-54.857-24.571-54.857-54.857v-109.714c0-30.286 24.571-54.857 54.857-54.857h237.714v-237.714c0-30.286 24.571-54.857 54.857-54.857h109.714c30.286 0 54.857 24.571 54.857 54.857v237.714h237.714c30.286 0 54.857 24.571 54.857 54.857z"
                        ></path>
                        </svg>
                        <span class="departments-text14"><a href="/application?department=<%= departments[i].id %>">View Application</a></span>
                    </button>
                </div>
            <% } %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
